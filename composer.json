{
    "name": "tenolo/translation-bundle",
    "description": "Adds a translation manager to teneolo components.",
    "keywords": [
        "translation",
        "database",
        "tenolo"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "homepage": "http://tenolo.de",
    "authors": [
        {
            "name": "tenolo - Robert Bernis, Johannes Herberhold GbR",
            "email": "info@tenolo.de",
            "homepage": "http://tenolo.de"
        },
        {
            "name": "Nikita Loges",
            "email": "nl@tenolo.de",
            "homepage": "http://tenolo.de"
        }
    ],
    "require": {
        "php": "~5.4",

        "symfony/http-kernel": "~2.7",
        "symfony/config": "~2.7",
        "symfony/dependency-injection": "~2.7",
        "symfony/form": "~2.7",
        "symfony/validator": "~2.7",
        "symfony/event-dispatcher": "~2.7",
        "symfony/translation": "~2.7",
        "symfony/routing": "~2.7",
        "symfony/http-foundation": "~2.7",
        "symfony/options-resolver": "~2.7",
        "symfony/twig-bundle": "~2.7",

        "doctrine/orm": "~2.5",
        "doctrine/common": "~2.5",

        "shapecode/repository-as-a-service": "~1.0",

        "tenolo/core-bundle": "1.1.*@dev",
        "tenolo/doctrine-table-prefix-bundle": "1.1.*@dev",
        "tenolo/admin-control-panel-bundle": "1.1.*@dev"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "~2.7|~3.0"
    },
    "autoload": {
        "psr-4": {
            "Tenolo\\Bundle\\TranslationBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tenolo\\Bundle\\TranslationBundle\\Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.tenolo.com/repo/private/"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-release/1.1": "1.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
