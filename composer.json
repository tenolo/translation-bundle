{
    "name": "tenolo/translation-bundle",
    "description": "Adds a translation manager to tenolo components.",
    "keywords": [
        "translation",
        "database",
        "tenolo"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "homepage": "http://tenolo.de",
    "authors": [
        {
            "name": "tenolo - Robert Bernis, Johannes Herberhold GbR",
            "email": "info@tenolo.de",
            "homepage": "http://tenolo.de"
        },
        {
            "name": "Nikita Loges",
            "email": "nl@tenolo.de",
            "homepage": "http://tenolo.de"
        }
    ],
    "require": {
        "php": "~5.6|~7.0",

        "symfony/framework-bundle": "~2.8|~3.0|~4.0",
        "symfony/http-kernel": "~2.8|~3.0|~4.0",
        "symfony/config": "~2.8|~3.0|~4.0",
        "symfony/dependency-injection": "~2.8|~3.0|~4.0",
        "symfony/form": "~2.8|~3.0|~4.0",
        "symfony/options-resolver": "~2.8|~3.0|~4.0",
        "symfony/translation": "~2.8|~3.0|~4.0",
        "symfony/doctrine-bridge": "~2.8|~3.0|~4.0",
        "symfony/filesystem": "~2.8|~3.0|~4.0",
        "symfony/finder": "~2.8|~3.0|~4.0",
        "symfony/console": "~2.8|~3.0|~4.0",

        "doctrine/orm": "~2.5",
        "doctrine/common": "~2.5",

        "tenolo/entity-bundle": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Tenolo\\Bundle\\TranslationBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tenolo\\Bundle\\TranslationBundle\\Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.tenolo.com/"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-release/1.0": "1.0-dev",
            "dev-release/2.0": "2.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
